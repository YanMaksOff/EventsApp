@model Event


<div class="card card-outline-primary m-1 p-1">
    <div class="bg-faded p-1">
        <h8>
           Дата события @Model.EventTime.ToShortDateString() / @Model.EventTime.ToShortTimeString()
        </h8>
        <partial name="_PlayerListPartial" model="@Model"></partial>
        <h4>
           @Model.EventName
        </h4>
        <span class="card-text p-1">
            @Model.EventText
        </span>
        
        <partial name="_ButtonAddPartial" model="@Model"></partial>
    </div>
    

   
</div>
